Algoritmo "creuza02"

Var r, d: real

Inicio
      escreval("Para comemorra meu aniversário, vou viajar para os EUA, me ajude a converter meu dinheiro de real para dólar")
      escreva("Quanto de dinheiro eu tenho em real?")
      leia(r)
      d <- r / 4.9
      escreva("Eu posso converter esses R$", r," para UR$", d," em dólares.")
Fimalgoritmo