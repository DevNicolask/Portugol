Algoritmo "creuza01"

Var n1, n2, idade: inteiro

Inicio
      escreval("Me ajude a descobrir quantas velas comprar pro meu niver.")
      escreval("Em qual ano estamos?")
      leia(n1)
      escreval("Em que ano eu nasci?")
      leia(n2)
      idade <- n1 - n2
      escreva("Entâo eu tenho", idade," anos")
Fimalgoritmo